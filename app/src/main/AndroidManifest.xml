<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sw_ss16.lc_app">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.Main">

        <activity android:name="com.sw_ss16.lc_app.ui.studyroom.ListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.sw_ss16.lc_app.ui.studyroom.StudyRoomDetailActivity"
            android:label=""
            android:parentActivityName="com.sw_ss16.lc_app.ui.studyroom.ListActivity"/>
        <activity android:name="com.sw_ss16.lc_app.ui.ViewSamplesActivity"
            android:label="Samples"
            android:parentActivityName="com.sw_ss16.lc_app.ui.studyroom.ListActivity"/>
        <activity android:name="com.sw_ss16.lc_app.ui.SettingsActivity"
            android:label="Settings"
            android:parentActivityName="com.sw_ss16.lc_app.ui.studyroom.ListActivity"/>

    </application>

</manifest>
